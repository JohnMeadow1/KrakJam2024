[sub_resource type="GDScript" id="GDScript_53iep"]
script/source = "extends Sprite2D


var previous_mouse_position := Vector2.ZERO
var value:= 0.0
var target_value:= 0.0
var is_on := false
var is_forced := false
@onready var button = $Button
signal toggled

func _ready():
	#button.pivot_offset = texture.get_size() * 0.5
	#button.position = -button.pivot_offset
	set_physics_process(false)

func _physics_process(delta):
	if is_forced: 
		value += 10.0 * delta
		value = min(value, PI)
		target_value = value
	else:
		var angle_difference = atan2(previous_mouse_position.cross(get_local_mouse_position()), previous_mouse_position.dot(get_local_mouse_position()) )
		previous_mouse_position = get_local_mouse_position()
		target_value += angle_difference
		target_value = clamp(target_value,0.0, PI)
		if target_value > value:
			value = min(value+0.2,target_value)
		else:
			value = max(value-0.2,target_value)
			
		value = clamp(value, 0.0, PI)
	button.rotation = value -PI*0.5
	if value > PI*0.9:
		is_on = true
	elif value < PI*0.1:
		is_on = false



func _on_button_down():
	if not is_forced:
		set_physics_process(true)
		previous_mouse_position = get_local_mouse_position()


func _on_button_up():
	if not is_forced:
		set_physics_process(false)

func toggle():

	is_on = true
	is_forced = true
	set_physics_process(true)
	
func unblock():
	is_forced = false
	set_physics_process(false)
"

